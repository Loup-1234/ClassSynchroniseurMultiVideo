        <definition>int64_t av_compare_mod</definition>
        <argsstring>(uint64_t a, uint64_t b, uint64_t mod)</argsstring>
        <name>av_compare_mod</name>
        <param>
          <type>uint64_t</type>
          <declname>a</declname>
        </param>
        <param>
          <type>uint64_t</type>
          <declname>b</declname>
