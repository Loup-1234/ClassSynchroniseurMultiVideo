        <type>int</type>
        <definition>int av_crc_init</definition>
        <argsstring>(AVCRC *ctx, int le, int bits, uint32_t poly, int ctx_size)</argsstring>
        <name>av_crc_init</name>
        <param>
          <type>AVCRC *</type>
          <declname>ctx</declname>
        </param>
        <param>
          <type>int</type>
