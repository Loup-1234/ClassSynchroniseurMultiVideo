        <definition>int av_dynamic_hdr_plus_from_t35</definition>
        <argsstring>(AVDynamicHDRPlus *s, const uint8_t *data, size_t size)</argsstring>
        <name>av_dynamic_hdr_plus_from_t35</name>
        <param>
          <type><ref refid="struct_a_v_dynamic_h_d_r_plus" kindref="compound">AVDynamicHDRPlus</ref> *</type>
