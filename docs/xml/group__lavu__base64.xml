        <definition>int av_base64_decode</definition>
        <argsstring>(uint8_t *out, const char *in, int out_size)</argsstring>
        <name>av_base64_decode</name>
        <param>
          <type>uint8_t *</type>
          <declname>out</declname>
        </param>
        <param>
          <type>const char *</type>
          <declname>in</declname>
        </param>
        <param>
